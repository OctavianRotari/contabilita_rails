<div class="row">
  <div class="index">
    <h2><%= @fuel_receipts_dashboard.company.name %></h2>
  </div>
</div>
<div class="row">
  <div class="invoices-by-year">
    <h3>Anno: <%= params[:year]%></h3>
    <h4><%= previos_year_fuel_receipts_company(@fuel_receipts_dashboard.company) %></h4>
    <h4><%= next_year_fuel_receipts_company(@fuel_receipts_dashboard.company) %></h4>
  </div>
</div>
<div class="row">
  <div class="index">
    <div id="fuel_receipts">
      <%= render partial: "fuel_receipts/fuel_receipts",  locals: { dashboard: @fuel_receipts_dashboard,  fuel_receipts: @fuel_receipts_dashboard.company_fuel_receipts_ord, fuel_receipts_calculator: @fuel_receipts_dashboard.calculator } %>
    </div>
  </div>
</div>
